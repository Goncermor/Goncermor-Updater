<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net8.0-windows10.0.22621.0</TargetFramework>
    <RootNamespace>Goncermor_Updater</RootNamespace>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UseWPF>true</UseWPF>
    <Platforms>AnyCPU;x64</Platforms>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Assets\Fonts\Big Space.ttf" />
    <None Remove="Assets\Fonts\Montserrat-Light.ttf" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Assets\Fonts\Big Space.ttf" />
    <Resource Include="Assets\Fonts\Montserrat-Light.ttf" />
  </ItemGroup>

</Project>
